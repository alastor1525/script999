SS1 = false

spawn(function()
    while wait() do
        pcall(function()
            if _G.set.Mytic then
                if not game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Mirage Island") then
                    if not game:GetService("Workspace").Boats:FindFirstChild(_G.set.SelectB) then
                        buyb = TPP(CFrame.new(-375.1658935546875, 6.755728244781494, 5217.19970703125))
                        if (CFrame.new(-375.1658935546875, 6.755728244781494, 5217.19970703125).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                            if buyb then buyb:Stop() end wait(.1)
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyBoat",_G.set.SelectB)
                        end
                    elseif game:GetService("Workspace").Boats:FindFirstChild(_G.set.SelectB) then
                        if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
                           TPP(game:GetService("Workspace").Boats:FindFirstChild(_G.set.SelectB).VehicleSeat.CFrame * CFrame.new(0,2,0))
                        else
                            for i,v in pairs(game:GetService("Workspace").Boats:GetChildren()) do
                                if v.Name == _G.set.SelectB then
                                    repeat wait()
                                        if (v.VehicleSeat.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 and SS1 == false then
                                            TPB(_G.set.SelectB,CFrame.new(260.3658142089844, 17.747055053710938, 3543.2646484375))
                                            SS1 = true
                                        elseif (CFrame.new(260.3658142089844, 17.747055053710938, 3543.2646484375).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                                            TPB(_G.set.SelectB,CFrame.new(29236.712890625, 17.74854850769043, 19706.36328125))
                                        elseif (CFrame.new(29236.712890625, 17.74854850769043, 19706.36328125).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                                            TPB(_G.set.SelectB,CFrame.new(260.3658142089844, 17.747055053710938, 3543.2646484375))
                                        end 
                                    until game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Mirage Island") or _G.set.Mytic == false
                                end
                            end
                        end
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Mirage Island") then
                    game:GetService("Workspace").Boats:FindFirstChild(_G.set.SelectB):Destroy()
                    TPP(game:GetService("Workspace").Map.MysticIsland.Center.CFrame * CFrame.new(0,300,0))
                    repeat wait(1)
                        game.StarterGui:SetCore("SendNotification", {
                            Title = "Notify"; -- the title (ofc)
                            Text = "Mirage not Found!"; -- what the text says (ofc)
                            Icon = ""; -- the image if u want. 
                            Duration = 3; -- how long the notification should in secounds
                        })
                    until not game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Mirage Island") or _G.set.Mytic == false
                end
            else
                TPB(_G.set.SelectB,game:GetService("Workspace").Boats:findFisrt(_G.set.SelectB).VehicleSeat.CFrame) 
            end
        end)
    end
end)

