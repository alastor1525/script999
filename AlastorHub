end)

Section9:Toggle("Esp Mirage Island",_G.set.MirageIslandESP,function(vu)
    MirageIslandESP = vu
    _G.set.MirageIslandESP = vu
    Update_Setting(getgenv()['MyName'])
    while MirageIslandESP do wait()
        UpdateIslandMirageESP()
    end
end)




