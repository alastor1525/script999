Section9:Label("Esp")

Section9:Toggle("Esp Islands",_G.set.IslandESPs,function(vu)
    IslandESPs = vu
    _G.set.IslandESPs = vu
    Update_Setting(getgenv()['MyName'])
    while IslandESPs do wait()
        UpdateIslandESPs() 
    end
end)


Section9:Button("Rejoin",function()
